{
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "standard",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-undef": "off"
      }
    }
  ],
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "error"
  }
}
